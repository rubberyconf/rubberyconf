    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch http api",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/httpapi/main.go",
            "args":[
                "serve",
                "-test.run",
                "TestWithHTTP"
            ],
            "showLog": false,
            "env": { 
                "name":"ENV", "value":"local"
            }
        },
        {
            "name": "Launch test package",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}"
        }
    ]
}